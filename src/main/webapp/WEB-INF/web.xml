<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>createAirlineServlet</servlet-name>
  	<display-name>createAirlineServlet</display-name>
  	<description></description>
  	<servlet-class>flyaway.servlets.createAirlineServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>createCustomerServlet</servlet-name>
  	<display-name>createCustomerServlet</display-name>
  	<description></description>
  	<servlet-class>flyaway.servlets.createCustomerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerLoginServlet</servlet-name>
  	<display-name>CustomerLoginServlet</display-name>
  	<description></description>
  	<servlet-class>flyaway.servlets.CustomerLoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>customerLogoutServlet</servlet-name>
  	<display-name>customerLogoutServlet</display-name>
  	<description></description>
  	<servlet-class>flyaway.servlets.customerLogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>createAirlineServlet</servlet-name>
  	<url-pattern>/createAirlineServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>createCustomerServlet</servlet-name>
  	<url-pattern>/createCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerLoginServlet</servlet-name>
  	<url-pattern>/CustomerLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>customerLogoutServlet</servlet-name>
  	<url-pattern>/customerLogoutServlet</url-pattern>
  </servlet-mapping>
</web-app>
